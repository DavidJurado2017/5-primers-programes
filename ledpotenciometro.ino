//********** Includes *************************************************************

//********** Variables ************************************************************
int sensorValue=0;

//********** Setup ****************************************************************
void setup()
{
   pinMode(3,OUTPUT);
}

//********** Loop *****************************************************************
void loop()
{
  sensorValue=analogRead(A0);  
  analogWrite(3,sensorValue/4);  
}

